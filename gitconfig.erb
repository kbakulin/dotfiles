[alias]
    s = status
    ci = commit
    cv = commit -v
    b = branch
    ba = branch -a
    co = checkout
    df = diff
    d = diff
    a = add
    l = log
    lg = log --graph --pretty=format:'%Cred%h%Creset %C(yellow)%d%Creset %s %Cgreen(%ar) %C(cyan)<%%an>%Creset' --abbrev-commit --date=relative
    ll = log --graph --pretty=format:'%Cred%h%Creset %C(yellow)%d%Creset %s %Cgreen(%ar) %C(cyan)<%%an>%Creset' --abbrev-commit --date=relative -20
    lp = log -p
    pl = pull --rebase
    ps = push
    m = merge --no-ff
    mm = merge
    f = flow
    st = stash
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color "branch"]
    current = yellow reverse
    local = green
    remote = red
[color "diff"]
    meta = yellow
    frag = cyan
    old = red
    new = green
[core]
    excludesfile = <%= ENV['HOME'] %>/.gitignore
[push]
    default = simple
[include]
    path = ~/.gitconfig.local
